node { 
    stage('Clone repository') { 
        git credentialsId: 'github-access', url: 'https://github.com/JANGSEOKHAN/jenkins1' 
    } 
 
    stage('Build image') { 
       dockerImage = docker.build("jangseokhan123/node-front:1.0") 
    } 
 
 stage('Push image') { 
        withDockerRegistry([ credentialsId: "docker-access", url: "" ]) { 
        dockerImage.push() 
        } 
    } 
}
